.messengers {
  display: flex;
  
  &__item {
    width: 47px;
    height: 32px;

    & > a {
      display: block;
      width: 100%;
      height: 100%;

      & > img {
        object-fit: contain;
        width: 100%;
      }
    }
    @media (--sm) {
      width: 35px;
      height: 24px;
    }
  }
}